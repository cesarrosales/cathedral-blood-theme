  "tokenColors": [
    { "scope": ["comment", "punctuation.definition.comment"], "settings": { "foreground": "#6f6669", "fontStyle": "italic" } },
    { "scope": ["string", "meta.string"], "settings": { "foreground": "#c3b9b2" } },
    { "scope": ["support.type.property-name.json"], "settings": { "foreground": "#8a6aa6" } },
    { "scope": ["constant.numeric"], "settings": { "foreground": "#ff6f6f" } },
    { "scope": ["keyword", "storage.type"], "settings": { "foreground": "#ff2a2a" } },
    { "scope": ["entity.name.function", "support.function"], "settings": { "foreground": "#ff6161" } },
    { "scope": ["variable", "meta.definition.variable"], "settings": { "foreground": "#d4cccc" } },
    { "scope": ["entity.name.type", "support.type"], "settings": { "foreground": "#b7a6b6" } },
    { "scope": ["constant.language", "support.constant"], "settings": { "foreground": "#b7a6b6" } },
    { "scope": ["punctuation", "meta.brace", "meta.delimiter"], "settings": { "foreground": "#8a8083" } },
    { "scope": ["markup.bold"], "settings": { "fontStyle": "bold" } },
    { "scope": ["markup.italic"], "settings": { "fontStyle": "italic" } }
  ]
}
